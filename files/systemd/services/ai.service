[Unit]
Description=webos - "%n"
Requires=pulseaudio.service default-webos.target
After=pulseaudio.service default-webos.target

[Service]
Type=simple
OOMScoreAdjust=-1000
ExecStart=/usr/sbin/aiservice
Restart=on-failure

[Install]
WantedBy=default-webos.target
